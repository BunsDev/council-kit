{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "council-ui#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"],
      "env": ["NEXT_PUBLIC_MAINNET_ALCHEMY_ID", "NEXT_PUBLIC_GOERLI_ALCHEMY_ID"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "format": { "outputs": [] },
    "format:check": { "outputs": [] }
  }
}
